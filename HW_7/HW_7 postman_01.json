						"exec": [
							"const person = pm.response.json();\r",
							"\r",
							"\r",
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"\r",
							"\r",
							" pm.test(\"age of person\", function () {\r",
							"    pm.expect(person.age).to.deep.equal(\"51\");\r",
							"});\r",
							"\r",
							" pm.test(\"age of children\", function () {\r",
							"    pm.expect(person.family.children[1][1]).to.deep.equal(12);\r",
							"    \r",
							"});\r",
							"\r",
							" pm.test(\"person has name\", function () {\r",
							"    pm.expect(person).to.have.property(\"name\");\r",
							"});\r",
							"\r",
							" pm.test(\"person salary 11000\", function () {\r",
							"    pm.expect(person).to.deep.include({\"salary\": 11000});\r",
							"});\r",
							"\r",
							" pm.test(\"age of 1 children include 12\", function () {\r",
							"    pm.expect(person.family.children[0]).to.deep.include(\"Alex\")});\r",
							"\r",
							"     pm.test(\"children are Array\", function () {\r",
							"    pm.expect(person.family.children[1]).to.be.a(\"Array\")});\r",
							"\r",
